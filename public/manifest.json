{
  "manifest_version": 3,
  "name": "HealthVibes",
  "version": "1.0.0",
  "description": "A health and fitness tracking application",
  "web_accessible_resources": [{
    "resources": [
      "assets/*",
      "images/*",
      "*.js",
      "*.css"
    ],
    "matches": ["<all_urls>"]
  }],
  "permissions": [
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
} 